{
    "env": {
        "browser": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "16.0"
      }
    },
    "rules": {
        "semi": [2, "always"],
        "space-before-function-paren": [0, {"anonymous": "always", "named": "always"}],
        "camelcase": 0,
        "no-return-assign": 0
    }
  }
  